# Agents configuration
#
# detector.campus_range controls which campuses are considered by the
# change detector. Use a comma-separated list or ranges (e.g., 1-54,76).
# Default "all" means no campus filtering.
#
[detector]
campus_range=all
TIME_WINDOW=65  # default=65
DELTA_SKIP_SECONDS=900  # Skip delta check if timestamp delta < this (900 = 15 min, 3600 = 1 hour)
# for the usr fetch
DRY_RUN=0
RATE_LIMIT_DELAY=3
DETECTOR_INTERVAL=60
DB_HOST=localhost
DB_PORT=3306
DB_USER=api42
DB_PASSWORD=api42
DB_NAME=api42
DELTA_SKIP_SECONDS=900 